## The function is currently defined as
function (x, y, z) 
{
    nx <- length(x)
    ny <- length(y)
    nz <- length(z)
    X <- array(dim = c(nx, ny, nz))
    Y <- X
    Z <- X
    for (i in 1:nx) for (j in 1:ny) for (k in 1:nz) {
        X[i, j, k] <- x[i]
        Y[i, j, k] <- y[j]
        Z[i, j, k] <- z[k]
    }
    grid <- list(X = X, Y = Y, Z = Z)
    return(grid)
  }
}
